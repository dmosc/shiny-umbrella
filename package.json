    "react-router-dom": "^5.2.1",
    "styled-components": "^5.3.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-react": "^7.20.6",
